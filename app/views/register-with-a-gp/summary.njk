{% extends 'layout-transactional.njk' %}

{% block pageTitle %}
  NHS.UK - Register at the Rose Medical Practice
{% endblock %}

{% block content %}
  <div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-three-quarters">

      <h1>Summary</h1>
      <p><strong>Your name:</strong> <span data-name="first-name"></span> <a href="/register-with-a-gp/page-4">Edit</a></p>
      <p><strong>Middle name:</strong> <span data-name="middle-name"></span> <a href="/register-with-a-gp/page-4">Edit</a></p>
      <p><strong>Last name:</strong> <span data-name="last-name"></span> <a href="/register-with-a-gp/page-4">Edit</a></p>
      <p><strong>NHS number: </strong> <span data-name="nhs-number"></span> <a href="/register-with-a-gp/page-3">Edit</a></p>
      <p><strong>Date of birth:</strong> <span data-name="dob-day"></span> <span data-name="dob-month"></span> <span data-name="dob-year"></span> <a href="/register-with-a-gp/page-5">Edit</a></p>
      <p><strong>Postcode: </strong> <span data-name="postcode"></span> <a href="/register-with-a-gp/page-6">Edit</a></p>
      <a href="/register-with-a-gp/page-2">Start again</a>

    </div>
  </div>
{% endblock %}
